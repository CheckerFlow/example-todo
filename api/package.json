{
  "private": true,
  "name": "api",
  "version": "0.0.0",
  "dependencies": {
    "@prisma/photon": "2.0.0-preview018.2",
    "@hammerframework/api": "^0.0.1-alpha.13.3"
  },
  "devDependencies": {
    "prisma2": "2.0.0-preview018.2",
    "@hammerframework/hammer-dev-server": "^0.0.1-alpha.12.5"
  },
  "scripts": {
    "dev": "yarn hammer-dev-server",
    "db:up": "yarn prisma2 lift up && yarn prisma2 generate",
    "db:save": "yarn prisma2 lift save",
    "db:seed": "node prisma/seeds.js",
    "build": "NODE_ENV=production babel src --out-dir dist"
  }
}
